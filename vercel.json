{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "functions": {
    "api/main.py": {
      "runtime": "python3.9"
    }
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_BASE_URL": "/api"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}